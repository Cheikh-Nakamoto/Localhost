<script type="text/javascript">
    $(function(){
        $(document).ready(function() {
            var downloadURL = function downloadURL(url) {
                window.location = url;
            };
            function getParameterByName(name, url) {
                if (!url) url = window.location.href;
                name = name.replace(/[\[\]]/g, "\\$&");
                var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                    results = regex.exec(url);
                if (!results) return null;
                if (!results[2]) return '';
                return decodeURIComponent(results[2].replace(/\+/g, " "));
            }                
            if (getParameterByName("download") != null) {
                setTimeout(function(){
                    dataLayer.push({event: 'app_download', product: 'videodownloaderplus'});
                    downloadURL("https://dl.4kdownload.com/app/4kvideodownloaderplus_1.10.5_online.exe?source=website");
                }, 500);
            }
        });
    });
</script>




