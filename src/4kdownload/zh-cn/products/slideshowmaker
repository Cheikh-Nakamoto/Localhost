  <section class="products-section products-requirements">
    <div class="products-requirements__item">
      <img width="40" height="40" src="https://static.4kdownload.com/main/img/redesign-v2/products-page/icons/ic_system.6ba96137d93a.svg" loading="lazy" class="products-requirements__item-icon" alt="系统要求" />
      <p class="products-requirements__item-title">系统要求:</p>
      
    <p class="products-requirements__item-info" itemprop="operatingSystem">Windows 7及更新版本</p>
    
    <p class="products-requirements__item-info" itemprop="operatingSystem">macOS 10.13及更新版本</p>
    <p class="products-requirements__item-info" itemprop="operatingSystem">Ubuntu64位</p>

    </div>
    <div class="products-requirements__item" itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating">
      <img width="40" height="40" src="https://static.4kdownload.com/main/img/redesign-v2/products-page/icons/ic_star_rating.481b55b9f027.svg" loading="lazy" class="products-requirements__item-icon" alt="评分" />
