    <section class="section">
        <h2 class="title text--center">使用4K&nbsp;Video&nbsp;Downloader获取更多信息</h2>
        <div class="section__subsection">
            <div class="other-feature">
                <div class="other-feature__first-group">
                    <div class="card card--white other-feature__card">
                        <div class="other-feature__icon-with-badge">
                            <img width="48" height="48" src="https://static.4kdownload.com/main/img/redesign-v2/products-page/videodownloaderplus/features/icon/private.16611b33fb08.svg" alt="" class="other-feature__logo" loading="lazy"/>
