<script>
//automatic downloading

document.addEventListener('DOMContentLoaded', function() {
    setTimeout(() => {
        prodName = 'youtubetomp3',
        startAppDwld(window.prodSourceLink || 'https://dl.4kdownload.com/app/4kyoutubetomp3_5.7.5_online.exe?source=website', prodName);
      }, 1000)
});

// Twitter

document.getElementById('shareTwitter').onclick = function() {
    var textForShare = "查看4K Download網站的4K YouTube to MP3。在你的PC、macOS或Linux電腦上下載並試用軟體，你不會後悔的！通過4K Download軟體，你將能從YouTube下載音訊和視頻、將視訊轉換為MP3、創建幻燈片、下載Instagram相片以及更多。";
    window.open(
        'https://twitter.com/share?text=' + encodeURIComponent(textForShare) + '&amp;url=' + 'https://www.4kdownload.com/products/product-' + 'youtubetomp3',
        'Twitter-dialog',
        'width=626,height=436'
    );
    // Clicked the Share on Twitter button
    dataLayer.push({event: 'share', network: 'Twitter', target: window.location.href});
    return false;
}


// Reddit

document.getElementById('shareReddit').onclick = function() {
    window.open(
        'https://www.reddit.com/submit?url=' + encodeURIComponent('https://www.4kdownload.com/products/product-' + 'youtubetomp3'),
        'width=626,height=436'
    );
    // Clicked the Share on Reddit button
    dataLayer.push({event: 'share', network: 'Reddit', target: window.location.href});
    return false;
}


</script>


        <!-- Social -->

        <script data-id="twitter-social-script" type="text/delayscript">
        window.twttr = (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0],
                t = window.twttr || {};
            if (d.getElementById(id)) return t;
            js = d.createElement(s);
            js.id = id;
            js.src = "https://platform.twitter.com/widgets.js";
            fjs.parentNode.insertBefore(js, fjs);

            t._e = [];
            t.ready = function(f) {
                t._e.push(f);
            };

            return t;
        }(document, "script", "twitter-wjs"));
        </script>
        <!-- End Social -->
